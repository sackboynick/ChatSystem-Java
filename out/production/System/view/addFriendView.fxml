<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="600.0" prefWidth="1048.0" xmlns="http://javafx.com/javafx/" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.AddFriendController">
   <children>
      <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
         <children>
            <Label layoutX="341.0" layoutY="43.0" text="Add a new Friend">
               <font>
                  <Font size="42.0" />
               </font>
            </Label>
            <TableView fx:id="userTableView" layoutX="58.0" layoutY="201.0" prefHeight="365.0" prefWidth="820.0">
               <columns>
                  <TableColumn fx:id="username" prefWidth="819.0" text="Username" />
               </columns>
            </TableView>
            <Label layoutX="58.0" layoutY="42.0" text="Your username:" underline="true" />
            <Label fx:id="usernameLabel" layoutX="177.0" layoutY="42.0" text="usernameLabel" />
            <TextField fx:id="searchUser" layoutX="257.0" layoutY="123.0" prefHeight="36.0" prefWidth="621.0" />
            <Label layoutX="111.0" layoutY="128.0" text="Username:">
               <font>
                  <Font size="20.0" />
               </font>
            </Label>
            <Button layoutX="905.0" layoutY="124.0" mnemonicParsing="false" onAction="#searchUser" prefHeight="34.0" prefWidth="121.0" text="SEARCH" />
            <Button layoutX="897.0" layoutY="50.0" mnemonicParsing="false" onAction="#back" text="BACK">
               <font>
                  <Font size="19.0" />
               </font>
            </Button>
            <Button layoutX="897.0" layoutY="353.0" mnemonicParsing="false" onAction="#addFriend" prefHeight="61.0" prefWidth="137.0" text="ADD FRIEND" textAlignment="CENTER" wrapText="true" />
         </children>
      </AnchorPane>
   </children>
</VBox>
