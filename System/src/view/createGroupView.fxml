<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="600.0" prefWidth="1048.0" xmlns="http://javafx.com/javafx/" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.CreateGroupController">
   <children>
      <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
         <children>
            <Label layoutX="411.0" layoutY="43.0" text="Create Group">
               <font>
                  <Font size="42.0" />
               </font>
            </Label>
            <Label layoutX="58.0" layoutY="42.0" text="Your username:" underline="true" />
            <Label fx:id="usernameLabel" layoutX="177.0" layoutY="42.0" text="usernameLabel" />
            <TextField fx:id="groupName" layoutX="353.0" layoutY="283.0" prefHeight="34.0" prefWidth="399.0" />
            <Label layoutX="245.0" layoutY="288.0" text="Name:">
               <font>
                  <Font size="20.0" />
               </font>
            </Label>
            <Button layoutX="353.0" layoutY="439.0" mnemonicParsing="false" onAction="#createGroup" prefHeight="65.0" prefWidth="399.0" text="CREATE GROUP">
               <font>
                  <Font size="37.0" />
               </font></Button>
            <Button layoutX="897.0" layoutY="50.0" mnemonicParsing="false" onAction="#onBack" text="BACK">
               <font>
                  <Font size="19.0" />
               </font>
            </Button>
         </children>
      </AnchorPane>
   </children>
</VBox>
