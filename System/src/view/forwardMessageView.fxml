<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="600.0" prefWidth="1048.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.ForwardMessageController">
   <children>
      <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
         <children>
            <Label layoutX="382.0" layoutY="42.0" text="Friends List">
               <font>
                  <Font size="42.0" />
               </font>
            </Label>
            <TableView fx:id="friendshipTableView" layoutX="57.0" layoutY="122.0" prefHeight="296.0" prefWidth="897.0">
               <columns>
                  <TableColumn fx:id="friendUsername" prefWidth="403.0" text="Friend Username" />
                  <TableColumn fx:id="friendFullName" minWidth="0.0" prefWidth="416.0" text="Full name" />
               </columns>
            </TableView>
            <Label layoutX="58.0" layoutY="42.0" text="Your username:" underline="true" />
            <Label fx:id="usernameLabel" layoutX="177.0" layoutY="42.0" text="usernameLabel" />
            <Label layoutX="76.0" layoutY="524.0" text="Text:">
               <font>
                  <Font size="20.0" />
               </font>
            </Label>
            <Button layoutX="738.0" layoutY="477.0" mnemonicParsing="false" onAction="#forwardMessage" prefHeight="34.0" prefWidth="198.0" text="FORWARD MESSAGE" />
            <Button layoutX="897.0" layoutY="50.0" mnemonicParsing="false" onAction="#onBack" text="BACK">
               <font>
                  <Font size="19.0" />
               </font>
            </Button>
            <Label fx:id="sender" layoutX="159.0" layoutY="456.0" text="Label" />
            <Label layoutX="71.0" layoutY="452.0" text="Sender:">
               <font>
                  <Font size="20.0" />
               </font>
            </Label>
            <Label fx:id="text" layoutX="159.0" layoutY="529.0" text="Label" />
         </children>
      </AnchorPane>
   </children>
</VBox>
